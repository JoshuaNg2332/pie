{
    "$schema": "https://turborepo.org/schema.json",
    "baseBranch": "origin/master",
    "pipeline": {
        "build": {
            "dependsOn": ["^build"]
        },
        "test": {
            "dependsOn": ["build"],
            "outputs": [],
            "inputs": ["src/**"]
        },
        "lint": {
            "outputs": []
        },
        "deploy": {
            "dependsOn": [
                "build", "test", "lint"
            ],
            "outputs": []
        },
        "dev": {
            "dependsOn": ["build"]
        }
    }
}
